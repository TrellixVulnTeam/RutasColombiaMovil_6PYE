{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ],
        "urls": [          
          "https://maps.googleapis.com/maps/api/js/**",    
          "https://maps.googleapis.com/maps/**",
          "https://maps.googleapis.com/maps/api/js/DirectionsService.Route",
          "https://maps.googleapis.com/maps/api/js?v=3&sensor=false&libraries=places&key=AIzaSyDBNR37FRIeI7ixrWSOFK9QF_SkM9WVTMc",          
          "https://maps.googleapis.com/maps/api/js?v=3&sensor=false&amp;libraries=places&key=AIzaSyDBNR37FRIeI7ixrWSOFK9QF_SkM9WVTMc"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "sitios-api",
      "urls": [        
        "https://apps.diegopedroza.co:11005/servicios_rc/**"
      ],
      "cacheConfig": {
        "maxSize": 20,
        "maxAge": "2h",
        "timeout": "50s",
        "strategy": "freshness"
      }      
    }
  ]
}
